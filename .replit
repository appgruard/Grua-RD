modules = ["nodejs-20", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["openjdk17", "android-tools"]

[[ports]]
localPort = 5000
externalPort = 80

[agent]
integrations = ["javascript_database:1.0.0"]

[workflows]

[[workflows.workflow]]
name = "Start application"
mode = "sequential"
author = 53119234

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
